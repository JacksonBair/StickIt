{
	"manifest_version": 3,
	"name": "StickIt",
	"description": "StickIt Notes for the webrowser",
	"version": "1.0",
	"icons": {
		"16": "icon.png",
		"48": "icon.png",
		"128": "icon.png"
	},
	"permissions": ["storage", "activeTab", "scripting", "windows"],
	"action": {
		"default_icon": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		},
		"default_title": "StickIt",
		"default_popup": "pages/popup.html"
	},
	"background": {
		"service_worker": "background.js"
	}
}
